    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: main.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: main.py MNIST",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["--config", "experiments/reference/mnist/vae_fid.yaml"]
        },
        {
            "name": "Python: compiler.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/compiler.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: compiler.py clean",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/compiler.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["--clean", "True"]
        }
    ]
}